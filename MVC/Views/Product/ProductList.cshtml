@{
    ViewBag.Title = "Index";
}
<h2>Product List using Html.Action For Admin Action Methond = AdminList </h2>
<table cellpadding="0" cellspacing="0" class="grid" id="CustomerGrid">
    <tr>
        <th>
            ProductID
        </th>
        <th>
            Name
        </th>
        <th>
            Category
        </th>
        <th>
            Description
        </th>
        <th>
            Price
        </th>
        <th></th>
    </tr>
    @Html.Action("AdminList")


</table>

<br />
<br />
<br />
<h2>Product List using  Html.RenderAction For Customer Action Methond = CustList </h2>
<table class="table">
    <tr>
        <th>
            ProductID
        </th>
        <th>
            Name
        </th>
        <th>
            Category
        </th>
        <th>
            Description
        </th>
        <th>
            Price
        </th>
        <th></th>

    </tr>
    @{
        Html.RenderAction("CustList");
    }


</table>
